/******************************************************************************
* 함 수 명 	: ac03110_onload
* 함수설명 	: 폼변수 호출 후 함수
*****************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var arrDataSet = Array(1); 

		arrDataSet[0]  = "ds_balDiv";  	        // 수지구분

	
	// 조회조건으로	넘어갈 공통코드	정의.(화면대로 순서꼭 지켜주세요.)
	
	var arrGrpCd = Array(1);

		arrGrpCd[0]  = "BG007"; 					//공통코드구분

			
	// 사용여부	지정할 아큐먼트를 넘긴다.
	var arrUseYn =	Array(1);
		arrUseYn[0]  =	"Y";
			
	// T:전체,S:선택,X:없음 
	var arrModeFg =	Array(1);
		arrModeFg[0]  =	"X";

	
	// WHERE절 내용 "X":없음
	var arrWhere =	Array(1);
		arrWhere[0]  =	"X";//ABB_CD IN ('S','K')

		
	/* 메소드명     : ComboLoad
	 * 설명         : 공통코드 테이블을 조회하여 콤보박스를 생성한다.
	 * 메소드인수1  : DataSet명의 배열
	 * 메소드인수2  : 그룹코드번호의 배열
	 * 메소드인수3  : 캠퍼스구분 배열 ("T":전체, "C":공통만, "S":서울, "K":경주)
	 * 메소드인수4  : 조건문의 배열 (1:사용, 0:사용안함)
	 * 메소드인수5  : 전체공란 첨부여부 "T": 전체, "S":선택, "X" : 데이터시작
	 * 메소드인수6  : WHERE절 내용 "X":없음
	 */

 	this.gfn_comboLoad(arrDataSet, arrGrpCd, arrUseYn, arrModeFg, arrWhere);
}

/********************************************************************************
 * 함 수 명 	: fn_postLoad()
 * 함수설명 	: 화면 로드 이벤트 후 함수
********************************************************************************/
this.fn_postLoad = function()
{
	this.gfn_bindCmmnFunction();
	
	this.div_search.cmb_balDiv.set_index(0);

	var currYy = this.gfn_getCurrentDate("YYYY");
	
	this.cfn_acntYy("ds_schAcntYy");
	
	this.cfn_acntUntCombo("ds_schAcntUnt",currYy,"","","1","ac03010"); //회계단위콤보호출
	
	this.div_search.div_Yymm_Fr.setData(this.gfn_getCurrentDate("YYYYMM"));
	this.div_search.div_Yymm_To.setData(this.gfn_getCurrentDate("YYYYMM"));
	
}


/********************************************************************************
 * 함 수 명 	: fn_preSearch
 * 함수설명 	: 조회 전 처리 함수
********************************************************************************/
this.fn_preSearch = function()
{
	if(this.div_search.div_Yymm_Fr.getData() == "" || this.div_search.div_Yymm_Fr.getData() == null){
		this.gfn_message("A","기준월을 입력하세요.");
		this.div_search.div_Yymm_Fr.setFocus();
		return false;
	}
	if(this.div_search.div_Yymm_To.getData() == "" || this.div_search.div_Yymm_To.getData() == null){
		this.gfn_message("A","기준월을 입력하세요.");
		this.div_search.div_Yymm_To.setFocus();
		return false;
	}
	
	return true;
}
 
/********************************************************************************
 * 함 수 명 	: btn_search_onclick
 * 함수설명 	: 조회버튼 클릭이벤트
********************************************************************************/
this.btn_search_onClick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	
	this.fn_search();
	
}


/********************************************************************************
 * 함 수 명 	: fn_search
 * 함수설명 	: 조회 함수
********************************************************************************/
this.fn_search = function()
{
	if(this.fn_preSearch()){	
		
		var vAcntCdFr = this.div_search.edt_acntCd.value;
		var vAcntCdTo = this.div_search.edt_acntCdTo.value;
		
		if (!this.gfn_isNull(this.div_search.edt_acntCd00.value)) {
			vAcntCdFr = this.div_search.edt_acntCd00.value;
		} 
		if (!this.gfn_isNull(this.div_search.edt_acntCd00.value)) {
			vAcntCdTo = this.div_search.edt_acntCdTo00.value;
		} 

		
		if ( this.gfn_isNull(vAcntCdFr) && this.gfn_isNull(vAcntCdTo) ) {
			vAcntCdFr = "000000";
			vAcntCdTo = "999999";
		}else if( !this.gfn_isNull(vAcntCdFr) && !this.gfn_isNull(vAcntCdTo) ){//계정과목 둘다 있음
		}else {
		
			if (!this.gfn_isNull(vAcntCdFr)) {
				vAcntCdTo = vAcntCdFr;
			} else {
				vAcntCdFr = vAcntCdTo;
			}
		}
		
		if(vAcntCdTo.substr(4,2) == "00") {
			vAcntCdTo = vAcntCdTo.substr(0,4) + "99";
		}
		
		
		
		var strArg  = "acntYy=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.spn_acntYy.value));
			strArg += "acntUnt=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.cmb_schAcntUnt.value));
			strArg += "balDiv=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.cmb_balDiv.value));
			strArg += "acntCdFr=" +  nexacro.wrapQuote(vAcntCdFr);
			strArg += "acntCdTo=" +  nexacro.wrapQuote(vAcntCdTo);
			strArg += "budgOrgId=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.edt_budgOrgId.value));
			strArg += "slipCfmFrMm=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.div_Yymm_Fr.getData()));
			strArg += "slipCfmToMm=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.div_Yymm_To.getData()));

		this.gfn_transaction("SEARCH",
							 "txComUrl::com/admi/ac03/selectAc03110MainList.do",
							 "",
							 "ds_mainList=resultList",
							 strArg,
							 "fn_callBack"
							 );
		}
}

/********************************************************************************
 * 함 수 명 	: fn_postSearch
 * 함수설명 	: 조회 후 처리 함수
********************************************************************************/
this.fn_postSearch = function()
{
	this.gfn_getDatasetCount(this.ds_mainList, this.sta_mainList);
}
