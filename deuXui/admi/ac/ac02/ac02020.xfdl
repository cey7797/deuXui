			this.gfn_message("A","접수구분이 일반 -> 분할로 변경되었습니다.");
			this.fn_Search();
		}
		else if(trId == "TABPAGE4_SAVE")
		{
			this.fn_tab01_tabpage4_save_post();
		}
		else if(trId == "SAVE_DESC")
		{
			this.fn_postSave();
		}
		else if(trId == "VOU_DETAIL_SAVE")
		{
			this.fn_vouDetail_postSave();
		}
		else if(trId == "VOU_DETAIL_DEL")
		{
			this.fn_vouDetail_postDel();
		}

 	}
