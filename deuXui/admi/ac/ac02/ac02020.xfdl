this.fn_tabList = function(strArg, strIndex)
{	
	var trId = "";
	var selectUrl = "";
	var dataSet = "";
	
	if(strIndex == 0)	//계정내역
	{
		//this.fn_SubSearch();
		return false;
	}
	else if(strIndex == 1)	//증빙내역
	{
		trId = "TABPAGE2_SEARCH";
		selectUrl = "txComUrl::com/admi/ac02/selectAc02020_t02_01.do";
		dataSet = "ds_ac_bill_evdc";
	}
	else if(strIndex == 2)	//원천세
	{
		trId = "TABPAGE3_SEARCH";
		selectUrl = "txComUrl::com/admi/ac02/selectAc02020_t03_01.do";
		dataSet = "ds_ac_inc_evdc";
	}
	else if(strIndex == 3)	//계좌내역(입금/지급)
	{
		var strArg  = "acntYy=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_vou.getColumn(this.ds_ac_vou.rowposition, "acntYy")));
			strArg += " acntUnt=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_vou.getColumn(this.ds_ac_vou.rowposition, "acntUnt")));
			strArg += " vouNo=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_vou.getColumn(this.ds_ac_vou.rowposition, "vouNo")));
			strArg += " slipNo=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_vou.getColumn(this.ds_ac_vou.rowposition, "slipNo")));
			//strArg += " vouKndType=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(vouKndType));
		
		
		var vouKndType = this.gfn_getNullToEmpty(this.ds_ac_vou.getColumn(this.ds_ac_vou.rowposition, "vouKnd"));
		
		if(vouKndType == 11 || vouKndType == 22)	//입금1
		{
			//this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(this.gfn_getNullToEmpty(vouKndType.substring(0,1)));
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(1);
			
			selectUrl = "txComUrl::com/admi/ac02/selectAc02020_t04_01.do";
		}
		else if(vouKndType == 12 || vouKndType == 21) //지급2
		{
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(2);
			
			selectUrl = "txComUrl::com/admi/ac02/selectAc02020_t04_02.do";
		}
		//대체
		else if(vouKndType == 31 || vouKndType == 32 || vouKndType == 33)
		{
			this.ds_ac_rcpt_desc_pymt.clearData();
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(1);
			return false;
		}
		else
		{
			this.ds_ac_rcpt_desc_pymt.clearData();
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(1);
			return false;
		}
		
		trId = "TABPAGE4_SEARCH";
		
		dataSet = "ds_ac_rcpt_desc_pymt";
		
	}else if(strIndex == 4){ //분개내역
	
		trId = "TABPAGE5_SEARCH";
		selectUrl = "txComUrl::com/admi/ac02/selectAc02030_t01_01.do";
		dataSet = "ds_ac_slip_det_drcr_10=resultList ds_ac_slip_det_drcr_20=resultList1";
	}
	else
	{
		return false;
	}
	
	var strDataset;
	
	if(strIndex != 4){
		strDataset = dataSet + "=resultList";
	}else{
		strDataset = dataSet;
	}
	
	this.gfn_transaction(trId,
						 selectUrl,
						 "",
						 strDataset,
						 strArg,
						 "fn_callBack");
				
}


