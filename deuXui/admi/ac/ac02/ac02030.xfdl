this.fn_tabList = function(strArg, strIndex)
{	
	var trId = "";
	var selectUrl = "";
	var dataSet = "";
	
// 	if(strIndex == 0)	//계정내역
// 	{
// 	
// 	}
// 	else 
	if(strIndex == 1)	//증빙내역
	{
		trId = "TABPAGE2_SEARCH";
		selectUrl = "txComUrl::com/admi/ac02/selectAc02030_t02_01.do";
		dataSet = "ds_ac_bill_evdc";
	}
	else if(strIndex == 2)	//원천세
	{
		trId = "TABPAGE3_SEARCH";
		selectUrl = "txComUrl::com/admi/ac02/selectAc02030_t03_01.do";
		dataSet = "ds_ac_inc_evdc";
	}
	else if(strIndex == 3)	//계좌내역(입금/지급)
	{
		var strArg  = "acntYy=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "acntYy")));
			strArg += " acntUnt=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "acntUnt")));
			strArg += " vouNo=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "vouNo")));
			strArg += " slipNo=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "slipNo")));
			//strArg += " vouKndType=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(vouKndType));
		
		
		var vouKndType = this.gfn_getNullToEmpty(this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "vouKnd"));
		
		if(vouKndType == 11 || vouKndType == 22)	//입금1
		{
			//this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(this.gfn_getNullToEmpty(vouKndType.substring(0,1)));
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(1);
			selectUrl = "txComUrl::com/admi/ac02/selectAc02020_t04_01.do";
		}
		else if(vouKndType == 12 || vouKndType == 21) //지급2
		{
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(2);
			selectUrl = "txComUrl::com/admi/ac02/selectAc02020_t04_02.do";
		}
		//대체
		else if(vouKndType == 31 || vouKndType == 32 || vouKndType == 33)
		{
			this.ds_ac_rcpt_desc_pymt.clearData();
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(1);
			return false;
		}
		else 
		{
			this.ds_ac_rcpt_desc_pymt.clearData();
			this.tab01.tabpage4.grid_ac_rcpt_pymt.set_formatid(1);
			return false;
		}
		
		trId = "TABPAGE4_SEARCH";
		
		dataSet = "ds_ac_rcpt_desc_pymt";
	}
	else
	{
		return true;
	}
	
	this.gfn_transaction(trId,
						 selectUrl,
						 "",
						 dataSet + "=resultList",
						 strArg,
						 "fn_callBack");
				
}



















