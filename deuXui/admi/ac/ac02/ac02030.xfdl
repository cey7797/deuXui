this.btn_ProcCd_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//전표 체크
	if(this.ds_ac_slip.rowcount <= 0)
	{
		this.gfn_message("A", "조회된(처리할) 전표가 없습니다.");
		return false;
	}
	
	/*
				
		회계전자결재승인63 : Y : 전표확정51
		전표확정51 : C : 전표확정취소
		
	*/
	
	//처리코드	
	inProcCd = "";
	
	inProcCd = this.ds_cmb_c_sta_cd.getColumn(this.ds_cmb_c_sta_cd.findRow("comCdNm", this.C_STA_CD.text), "procCd");
	
	//confirm
	var bOK = false;
	
	if ( inProcCd == "AC" ) {
		bOK = this.gfn_message("C", this.C_STA_CD.text + " 를 진행 하시겠습니까?\n\n전표취소 작업시 다시 취소할 수 없으며 재기안해야합니다.");
	} else {
		bOK = this.gfn_message("C", this.C_STA_CD.text + " 을(를) 진행 하시겠습니까?");
	}
        	
	if (bOK ==true)
	{
		if(this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition,"recvOrgId") == "20" ){
			//접수부서 - 구매팀
			this.fn_P_AC_STA_CD_PROC_PUCH(inProcCd);
		}else{
			//접수부서 - 경리팀
			this.fn_P_AC_STA_CD_PROC(inProcCd);
		}
		
	}
	else
	{
		return false;
	}
}

//진행상태 처리 SP 실행
this.fn_P_AC_STA_CD_PROC = function(inProcCd)
{
	/*
				
		회계전자결재승인63 : Y : 전표확정51
		전표확정51 : C : 전표확정취소
		
	*/
			
	var addRow = this.ds_P_AC_STA_CD_PROC.addRow();	
			
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "rowStatus", this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "rowStatus"));
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "acntYy", this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "acntYy"));
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "acntUnt", this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "acntUnt"));
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "vouNo", this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "vouNo"));
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "procCd", inProcCd);
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "userId", sMemberNo);
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "userIp", sIpAddr);
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "userPgm", "ac02030.xfdl");
	this.ds_P_AC_STA_CD_PROC.setColumn(addRow, "cfmDt", this.cal_fixDate.value);
			
	this.gfn_transaction("P_AC_STA_CD_PROC",
						 "txComUrl::com/admi/ac02/P_AC_STA_CD_PROC.do",
						 "resultList=ds_P_AC_STA_CD_PROC:u",
						 "ds_P_AC_STA_CD_PROC_result=resultList",
						 "",
						 "fn_callBack");
}

//진행상태 처리 SP 실행
this.fn_P_AC_STA_CD_PROC_PUCH = function(inProcCd)
{
	/*
				
		회계전자결재승인63 : Y : 전표확정51
		전표확정51 : C : 전표확정취소
		
	*/
			
	var addRow = this.ds_P_AC_STA_CD_PROC.addRow();	
			
