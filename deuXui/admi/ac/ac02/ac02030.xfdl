this.btn_Save_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.ds_ac_slip.rowposition < 0 ) {
		this.gfn_message("A", "선택된 행이 없습니다.");
		return;
	}

	//전표 접수자 처리
	if ( this.ds_ac_slip.getColumn(this.ds_ac_slip.rowposition, "slipAccEmpId") == sMemberNo ) {
		this.gfn_message("A", "전표접수자가 이미 본인으로 지정되어 있습니다.");
		return;
	}
	
	var strT = this.gfn_message("C", "전표접수자를 본인으로 변경하시겠습니까?");
	
	if ( strT ) {
	
		//var strArg = "memberNo=" + sMemberNo;
		
		this.ds_ac_slip.setColumn(this.ds_ac_slip.rowposition, "slipAccEmpId", sMemberNo);
		
		this.gfn_transaction("SLIP_ACC_EMP_SAVE",
							  "txComUrl::com/admi/ac02/saveSlipAccEmpId.do",
							  "resultList=ds_ac_slip:u",
							  "",
							  "",
							 "fn_callBack");
		}
}
