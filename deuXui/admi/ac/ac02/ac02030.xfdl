______________________________________________________________________________________________________________*/

this.fn_callBack = function(trId, errCd, errMsg)
{
	if(errCd < 0)
	{
		this.gfn_message("A", errMsg);
 	}
 	else
 	{
		if(trId=="LOAD")
		{
			this.fn_postLoad();
 		}
 		else if(trId == "SEARCH")
 		{
 			this.fn_postSearch();
 		}
 		else if(trId == "SUBSEARCH")
 		{
 			this.fn_postSubSearch();
 		}
 		
 		else if(trId == "TABPAGE1_SEARCH")	//계정내역
 		{
 			
		}
		else if(trId == "TABPAGE2_SEARCH")	//증빙내역
		{
			this.gfn_getDatasetCount(this.ds_ac_bill_evdc, this.tab01.tabpage2.ds_ac_bill_evdc_cnt);
		}
		else if(trId == "TABPAGE3_SEARCH")	//원천세
		{
			this.gfn_getDatasetCount(this.ds_ac_inc_evdc, this.tab01.tabpage3.ds_ac_inc_evdc_cnt);
		}
		else if(trId == "TABPAGE4_SEARCH")	//계좌내역(입금/지급)
		{
			this.gfn_getDatasetCount(this.ds_ac_rcpt_desc_pymt, this.tab01.tabpage4.ds_ac_rcpt_desc_pymt_cnt);
		}
		
		else if(trId == "ACNTUNTCOMBO"){
			this.div_search.S_ACNT_UNT.set_index(0)
		}
		else if(trId == "P_AC_STA_CD_PROC")
		{
			this.fn_postP_AC_STA_CD_PROC();
		}
		
		else if(trId == "SAVEDR")	//분개내역->차변 저장
 		{
 			var strMsg = "차변목록이 저장되었습니다.";
 			this.gfn_message("A", strMsg);
 			this.fn_SubSearch();
		}
		
		else if(trId == "SAVECR")	//분개내역->대변 저장
 		{
 			var strMsg = "대변목록이 저장되었습니다.";
 			this.gfn_message("A", strMsg);
 			this.fn_SubSearch();
		}
		else if(trId =="SLIP_ACC_EMP_SAVE") 
		{
			this.gfn_message("A", "처리되었습니다.");
			this.fn_Search();
		}
 	}
}




