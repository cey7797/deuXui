this.fn_callBack = function(trId, errCd, errMsg) 
{
	if(errCd < 0 ) {
		this.gfn_message("A", errMsg);
		return false;
	}
	
	if("LOAD" == trId) 
	{
		this.fn_membasCombo();
		
		this.chk_psnInfAgree.set_value("1");
		this.chk_juminInfAgree.set_value("1");
		
		this.ds_onlnCntrct.clearData();
		this.gfn_addRow(this.ds_onlnCntrct);	
		
		this.cmb_memberGb.set_index(0);
		this.cmb_memberType.set_index(0);
		
		this.cmb_memberGb.setFocus();
		
		this.cmb_membas.set_index(this.ds_membas.getRowCount());
	}
	else if("SAVE" == trId) 
	{
		this.fn_onlnPop();
	}
}
