/****************************************************************************
* 함 수 명 	: fn_preSave
* 함수설명 	: 확인전 체크 함수
*****************************************************************************/
this.fn_preSave = function() 
{
	var colNm = "memberGb$memberType$memberNmKor$birthDt$hpNo$endwCd$endwNm$enggAmt";
	var korNm = "회원구분$회원유형$성명$생년월일$핸드폰번호$기금$기금$약정총액";
	var cmpNm = "";
	var result = this.gfn_chkEssItem("ds_onlnCntrct", colNm, korNm, cmpNm);
	
	if (result[0] != "success") 
	{
		this.gfn_message("A", result[0]);
		this.ds_onlnCntrct.set_rowposition(result[1]);
		return false;	
	}
	
	//주소체크
	if(!this.gfn_isNull(this.edt_zip.value) && !this.gfn_isNull(this.edt_addr1.value))
	{
		if(this.gfn_isNull(this.edt_addr2.value))
		{
			this.gfn_message("A", "상세주소를 입력해 주세요.");
			return false;
		}
	}	
	//개인정보 수집이용 동의 체크
	if("1" != this.chk_psnInfAgree.value || "1" != this.chk_juminInfAgree.value) 
	{
		this.gfn_message("A", "개인정보 수집, 이용[약관]에 동의하셔야 합니다.");
		return false;
	}
	
	return true;
}

