* 함수설명 	: 기금팝업 callback
*****************************************************************************/
this.fn_endwCallBack = function(strID, rtnVal) 
{	
	
	if("close" == rtnVal || this.gfn_isNull(rtnVal)) {
		return false;
	}
	else 
	{
		this.ds_dummy.clear();
		this.ds_dummy.loadXML(rtnVal);
		
		
		
		//(약관정보)기금팝업
		if("afd02020_p01" == strID) 
		{
			this.edt_endwCd.set_value(this.ds_dummy.getColumn(0, "endwCd"));	//기금코드명
			this.edt_endwNm.set_value(this.ds_dummy.getColumn(0, "endwNm"));	//기금코드
			this.edt_mentFormDiv.set_value(this.ds_dummy.getColumn(0, "mentFormDiv"));//관리화면구분
			
		
			//기금을 값 선택 후 처리
			if(!this.gfn_isNull(this.edt_endwCd.value))
			{
				//기탁형태
				this.cmb_paymthd.set_enable(true);	
				this.cmb_paymthd.set_index(0);
				this.cmb_paymthd.setFocus();
				
				this.cmb_paymthd.set_value("01");//현금01				
				this.cmb_paymthd_onitemchanged();//기탁형태 변경 이벤트 호출
				
			
				//기금종류에 따라 DIV호출
				this.fn_endwDivCall(this.edt_mentFormDiv.value);
			}
			else
			{
				//기탁형태
				this.cmb_paymthd.set_enable(false);
				this.cmb_paymthd.set_value("");
			}
			
		}
	}
}

