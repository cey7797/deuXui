*****************************************************************************/
this.btn_excel01_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.div_search.div_Yymm_Fr.getData()!=this.div_search.div_Yymm_To.getData()){
		this.gfn_message("A", "1개월 단위로 급여공제 생성 가능합니다. 조회년월을 동일하게 하시기 바랍니다.");
		return;
	}
// 	if(!this.confirm(this.div_search.div_Yymm_To.getData()+" 데이터를 생성하시겠습니까?")){
// 		return false;
// 	}
	
	var chkYearAll = "";
	var strArg = "yymmFr=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.div_Yymm_Fr.getData(), ""));//조회시작년월
		strArg += "yymmTo=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.div_Yymm_To.getData(), ""));//조회년월
		strArg += " endYn=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_useYn.value, ""));//마감여부
		strArg += " endwCd=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_endwCd.value, ""));//기금구분
		//strArg += " memberNm=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_memberNm.value, ""));//성명
		strArg += " memberNo=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_sMemberNo.value, ""));
		strArg += " chkAll=" + nexacro.wrapQuote(chkYearAll);//전체년도 체크

	this.fn_changeData(strArg);//변동사항
	
	this.fn_changeDataDetail(strArg);//변동사항상세

	this.fn_accountancy(strArg);//경리팀제출

	this.fn_rcpmny01(strArg);//일반(지정)기탁|

	this.fn_rcpmny11(strArg);//일반기탁||

	var excelGrd = new Array();
		excelGrd[0] = this.grd_changeData; //변동사항
		excelGrd[1] = this.grd_changeDataDetail; //변동사항상세
		excelGrd[2] = this.grd_accountancy;//경리팀제출
		excelGrd[3] = this.grd_rcpmny01; //교직원일반(지정)1
		excelGrd[4] = this.grd_rcpmny11; //교직원일반2
	this.gfn_excelDown(excelGrd, this.div_search.div_Yymm_Fr.getData()+"_급여공제양식파일");
}

