 	this.gfn_excelDown(excelGrd, "급여공제관리");
}

/****************************************************************************
* 함 수 명 	: fn_search
* 함수설명 	: 조회함수
*****************************************************************************/
this.fn_search = function() 
{
	if(this.div_search.div_Yymm_Fr.getData()>this.div_search.div_Yymm_To.getData()){
		this.gfn_message("A", "조회년도를 확인하십시오.");
	}
	if(this.div_search.chkAll.value == true && this.gfn_isNull(this.div_search.edt_endwCd.value) == true && this.gfn_isNull(this.div_search.edt_sMemberNo.value) == true){
		this.gfn_message("A", "전체년도 조회시 기금구분 또는 이름이 필수입니다.");
		return;
	}
	
	var chkYearAll = "";
	if(this.div_search.chkAll.value == true){
		chkYearAll = "Y";
	}else{
		chkYearAll = "N";
	}

	var strArg = "yymmFr=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.div_Yymm_Fr.getData(), ""));//조회시작년월
		strArg += " yymmTo=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.div_Yymm_To.getData(), ""));//조회년월
		strArg += " endYn=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_useYn.value, ""));//마감여부
		strArg += " endwCd=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_endwCd.value, ""));//기금구분
		//strArg += " memberNm=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_memberNm.value, ""));//성명
		strArg += " memberNo=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_sMemberNo.value, ""));
		strArg += " chkAll=" + nexacro.wrapQuote(chkYearAll);//전체년도 체크
		
	
	this.gfn_transaction("SEARCH"
						,"txAdmUrl::com/admi/afd/afd02/selectFD02032_01.do"
						,""
						,"ds_rcpmny=resultList"
						,strArg
						,""
						,true);
}

