/****************************************************************************
* 함 수 명 	: fn_callBack
* 함수설명 	: callback함수
*****************************************************************************/
this.fn_callBack = function(trId, errCd, errMsg) 
{
	if(errCd < 0 ) 
	{
		this.gfn_message("A", errMsg);
		return;
	}
	
	//조회
	if(trId == "SEARCH") 
	{
		this.gfn_getDatasetCount(this.ds_rcpmny, this.sta_count);
	}
	//생성 전 데이터 확인함수
	else if(trId == "CHECK_DATA")
	{
		if(this.ds_rcpmny_chk.getColumn(0,"endDataCnt") > 0){
			this.gfn_message("A", this.div_search.div_Yymm_To.getData()+" 데이터에 마감된 데이터가 존재합니다.");
			return false;
		}

		if(this.ds_rcpmny_chk.getColumn(0,"dataCnt") > 0){
			if(!this.confirm("기존 데이터가 존재합니다. 삭제하고 재생성하시겠습니까?")){
				return false;
			}
		}
		this.fn_createData();
	}
	//생성 후
	else if(trId == "CREATE_DATA")
	{
		this.gfn_message("A", this.div_search.div_Yymm_To.getData()+" 데이터가 생성되었습니다.");
	}
	//저장
	else if(trId == "SAVE")
	{
		this.fn_postSaveSub();
	}
}

/****************************************************************************
* 함 수 명 	: fn_popCallBack
* 함수설명 	: 팝업 callback함수
*****************************************************************************/
this.fn_popCallBack = function(strID, rtnVal) 
{
	if("close" == rtnVal) 
	{
		return;
	}
		
	//기금팝업
	if("afd02020_p01" == strID) 
	{
		this.ds_dummy.loadXML(rtnVal);

		this.div_search.edt_endwCd.set_value(this.ds_dummy.getColumn(0, "endwCd"));
		this.div_search.edt_endwNm.set_value(this.ds_dummy.getColumn(0, "endwNm"));
	}
	else if("afd02020_p02" == strID) 
	{
		this.ds_dummy.loadXML(rtnVal);
		this.div_search.edt_sMemberNo.set_value(this.ds_dummy.getColumn(0, "memberNo"));
		this.div_search.edt_sMemberNmKor.set_value(this.ds_dummy.getColumn(0, "memberNmKor"));		
	}
}

