//일괄적용
this.btn_apply_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//전체 일때 조회조건이 한달이 아닐때
	if(this.div_search.div_Yymm_Fr.getData() != this.div_search.div_Yymm_To.getData() || this.div_search.chkAll.value == 1)
	{
		this.gfn_message("A", "지출일 일괄적용은 시작/종료년월이 같아야합니다.");
	}
	//일괄적용
	else
	{		
		
		for(var i=0; i< this.ds_rcpmny.rowcount ; i++)
		{
			this.ds_rcpmny.setColumn(i, "spenDt", this.cal_rcpmnySpenDt.value);
		}
		
		//지출일 일괄적용 후 저장
		if(this.confirm("지출일( " + this.cal_rcpmnySpenDt.text + " )을 일괄적용 하시겠습니까?"))
		{
			
			this.fn_save();
		}		
	}
}

