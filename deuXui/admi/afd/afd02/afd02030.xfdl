* 함수설명 	: 납부내역 조회함수
*****************************************************************************/
this.fn_search = function() 
{
	//no check
	if(this.div_search.All_Yymm.value == false)
	{
		var strArg = "rcpmnyFromDt=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cal_rcpmnyFromDt.value, "19000101"));
			strArg += " rcpmnyToDt=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cal_rcpmnyToDt.value, "21001231"));
			strArg += " paymentBankCd=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_sBankCd.value, "")); 
			strArg += " memberGb=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_sMemberGb.value, ""));
			strArg += " memberNo=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_sMemberNo.value, ""));
			strArg += " memberNm=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_sMemberNm.value, ""));
			strArg += " endwCd=" + nexacro.wrapQuote(this.gfn_nvl(this.edt_endwCd.value, ""));
	}
	//yes check
	else
	{
		var strArg = "rcpmnyFromDt=" + nexacro.wrapQuote("");
			strArg += " rcpmnyToDt=" + nexacro.wrapQuote("");
			strArg += " paymentBankCd=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_sBankCd.value, "")); 
			strArg += " memberGb=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_sMemberGb.value, ""));
			strArg += " memberNo=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_sMemberNo.value, ""));
			strArg += " memberNm=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_sMemberNm.value, ""));
			strArg += " endwCd=" + nexacro.wrapQuote(this.gfn_nvl(this.edt_endwCd.value, ""));
	}
	
	if(this.div_search.cal_rcpmnyFromDt.value>this.div_search.cal_rcpmnyToDt.value){
		this.gfn_message("A", "입금일자를 확인하십시오.");
		return false;
	}
	//전체연월 시 이름 필수
	if(this.div_search.All_Yymm.value == true && this.gfn_nvl(this.div_search.edt_sMemberNo.value, "") == "" 
	&& this.gfn_nvl(this.edt_endwCd.value, "") == ""
	&& this.gfn_nvl(this.div_search.edt_sMemberNm.value, "") == "")
	{
		this.gfn_message("A", "전체 년도 조회시 회원성명 또는 기금구분은 필수입니다.");
	}
	else
	{
// 		var strArg = "rcpmnyFromDt=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cal_rcpmnyFromDt.value, "19000101"));
// 			strArg += " rcpmnyToDt=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cal_rcpmnyToDt.value, "21001231"));
// 			strArg += " paymentBankCd=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_sBankCd.value, "")); 
// 			strArg += " memberDivCd=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.cmb_sMemberDivCd.value, ""));
// 			strArg += " memberNo=" + nexacro.wrapQuote(this.gfn_nvl(this.div_search.edt_sMemberNo.value, ""));
// 			strArg += " endwCd=" + nexacro.wrapQuote(this.gfn_nvl(this.edt_endwCd.value, ""));
		
		this.gfn_transaction("SEARCH"
							 ,"txAdmUrl::com/admi/afd/afd02/retrieveRcpmny.do"
							 ,""
							 ,"ds_rcpmny=resultList"
							 ,strArg
							 ,""
							 ,true);
	}
	
}

/****************************************************************************
* 함 수 명 	: fn_preSave
* 함수설명 	: 납부내역 저장 전 함수
*****************************************************************************/
this.fn_preSave = function() 
{
	if(!this.gfn_isUpdate(this.ds_rcpmny)){
		this.gfn_message("A", "변경된 데이터가 없습니다.");
		return false;
	}

	var colNm = "memberNo$enggMgnNo$rlPaymentPsn$paymentAmt$rcpmnyDt";
	var korNm = "약정회원$약정관리번호$입금인$납입금액$입금일자";
	var cmpNm = "$";
	var result = this.gfn_chkEssItem("ds_rcpmny", colNm, korNm, cmpNm);
	if (result[0] != "success") {
		this.gfn_message("A", result[0]);
		this.ds_rcpmny.set_rowposition(result[1]);
		if(result[3] != ""){
		
			this.objects[result[3]].set_focus();
			
		}
		return false;	
	}
	return true;
}

