this.fn_popCallBack = function(strID, rtnVal) 
{
	if("close" == rtnVal) {
		return;
	}
	
	//기금팝업
	if("afd02020_p01" == strID) 
	{
		this.ds_dummy.loadXML(rtnVal);

		this.edt_endwCd.set_value(this.ds_dummy.getColumn(0, "endwCd"));
		this.edt_endwNm.set_value(this.ds_dummy.getColumn(0, "endwNm"));
	}
	else if("afd02020_p02" == strID) 
	{
		this.ds_dummy.loadXML(rtnVal);

		this.edt_memberNo.set_value(this.ds_dummy.getColumn(0, "memberNo"));
 		this.edt_memberNmKor.set_value(this.ds_dummy.getColumn(0, "memberNmKor"));
 		
		//회원의 약정관리번호를 가져온다.
		this.fn_retrieveEnggMgnNo(this.edt_memberNo.value);
	}
	else if("afd02020_p02_s" == strID) 
	{
		this.ds_dummy.loadXML(rtnVal);
 		
		this.div_search.edt_sMemberNo.set_value(this.ds_dummy.getColumn(0, "memberNo"));
 		this.div_search.edt_sMemberNmKor.set_value(this.ds_dummy.getColumn(0, "memberNmKor"));
	}
}

/****************************************************************************
* 함 수 명 	: fn_callBack
* 함수설명 	: callback 함수
*****************************************************************************/
this.fn_callBack = function(trId, errCd, errMsg) 
{
	if(errCd < 0 ) {
		this.gfn_message("A", errMsg);
		return;
	}
	
	if("LOAD" == trId) 
	{
		this.fn_postLoad();		
	} 
	else if("MEMBAS" == trId) 
	{
		this.div_search.cmb_sMemberDivCd.set_index(0);
	}
	else if("ENGG_MGN_NO" == trId) 
	{
		
	} 
	else if("SAVE" == trId) 
	{
		this.gfn_message("A", "정상적으로 처리 되었습니다.");
		this.fn_search();
	}
	else if("SEARCH" == trId) 
	{
		this.gfn_getDatasetCount(this.ds_rcpmny, this.sta_count);
	}
	//약정정보 선택 후
	else if("SEARCH_01" == trId) 
	{
		//this.gfn_getDatasetCount(this.ds_rcpmny, this.sta_count);
		this.ds_rcpmny.setColumn(this.ds_rcpmny.rowposition, 'cnfirmYn', 0);
		this.ds_rcpmny.setColumn(this.ds_rcpmny.rowposition, 'rlPaymentPsn', this.ds_rcpmny.getColumn(this.ds_rcpmny.rowposition, 'memberNmKor'));
		this.ds_rcpmny.setColumn(this.ds_rcpmny.rowposition, 'paymentAmt', this.ds_afdCntrct.getColumn(0, 'enggAmt'));
		this.ds_rcpmny.setColumn(this.ds_rcpmny.rowposition, 'rcpmnyDt', this.ds_afdCntrct.getColumn(0, 'frstInspyPrearngeDt'));
		this.ds_rcpmny.setColumn(this.ds_rcpmny.rowposition, 'paymthd', 1);
		if(this.ds_afdCntrct.getColumn(0, 'paymthd') == 1)
		{//일회성
			this.ds_rcpmny.setColumn(this.ds_rcpmny.rowposition, 'paymentAmt', this.ds_afdCntrct.getColumn(0, 'enggAmt'));
		}
		else
		{//정기적, 급여공제
			this.ds_rcpmny.setColumn(this.ds_rcpmny.rowposition, 'paymentAmt', this.ds_afdCntrct.getColumn(0, 'mtInspyAmt'));
		}
	}
}

