/****************************************************************************
* 함 수 명 	: btn_del_onclick
* 함수설명 	: 삭제버튼
*****************************************************************************/
this.btn_del_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	var nCount = 0;
	
	for(i=this.ds_rcpmny.getRowCount(); i>=0; i--)
	{
		if(this.ds_rcpmny.getColumn(i,'chk') == '1')
		{
			nCount ++;
		}
	}
	
	if(nCount == 0)
	{
		this.gfn_message("A","삭제할 항목을 선택하여 주세요.");
		return false;
	}
	

	if(!this.confirm("선택한 항목을 삭제하시겠습니까?"))
	{
		return false;
	}

	nCount = 0;
	
	for(i=this.ds_rcpmny.getRowCount(); i>=0; i--)
	{
		if(this.ds_rcpmny.getColumn(i,'chk') == '1')
		{
			if(this.ds_rcpmny.getColumn(i, 'rowStatus') == '2')
			{
				this.ds_rcpmny.deleteRow(i);
			}
			else
			{
				this.ds_rcpmny.setColumn(i, 'rowStatus', '8');
				this.ds_rcpmny.setColumn(i, 'delYn', '1');
				nCount++;
			}
		}
	}
	trace(this.ds_rcpmny.saveXML());
	if(nCount >0)
	{
		this.fn_save();
	}
}

