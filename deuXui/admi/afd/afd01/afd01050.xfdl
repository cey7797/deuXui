		essGrd[0] = this.grd_main01;
 	
 	this.gfn_essDataset(essGrd);
	this.gfn_bindCmmnFunction();
 	
		
}

/****************************************************************************
* 함 수 명 	: fn_membasCombo
* 함수설명 	: 회원구분코드 조회(탭-회원등급)
*****************************************************************************/
