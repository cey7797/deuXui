			this.tab_afd.tp_hlrt.fn_search();
		}
		
	}
	else if(this.tab_afd.tabindex == 4) { // DM현황
		if(this.tab_afd.tp_dm.ds_afdDmsndng) {
			this.tab_afd.tp_dm.fn_search();
		}
	}
}

