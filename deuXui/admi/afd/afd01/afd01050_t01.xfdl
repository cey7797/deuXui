			this.edt_zip.set_value(this.ds_rtnZipVal.getColumn(0, "zip"));
			this.edt_addr1.set_value(this.ds_rtnZipVal.getColumn(0,"addr"));
		}
		else if("SEARCH_WRCZIP" == strID) 
