 	// WHERE절 내용 "X":없음
 	var arrWhere =	Array(4);
 		arrWhere[0]  =	"X";//ABB_CD IN ('S','K')
 		arrWhere[1]  =	"X";//ABB_CD IN ('S','K')
 		arrWhere[2]  =	"X";//ABB_CD IN ('S','K')
 		arrWhere[3]  =	"X";//ABB_CD IN ('S','K')
