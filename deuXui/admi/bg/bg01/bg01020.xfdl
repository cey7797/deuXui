********************************************************************************/
this.fn_preSearch = function()
{
	// 값이 없거나 길이가 4가 아니라면
	if(this.gfn_isNull(this.div_search.spn_acntYy.value) || this.gfn_nvl(new String(this.div_search.spn_acntYy.value).length, 0) != 4) {
		this.gfn_message("A", "회계연도를 확인해 주세요.");
		return false;
	}
	
	return true;
}

/********************************************************************************
 * 함 수 명 	: fn_search
 * 함수설명 	: '계정내역'조회 함수
********************************************************************************/
this.fn_search = function()
{
	if(this.fn_preSearch()){
		var strArg  = "acntYy=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.spn_acntYy.value));
			strArg += " acntUnt=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.cmb_schAcntUnt.value));
			strArg += " acntNm=" + nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.edt_acntNm.value));
		
		this.gfn_transaction("SEARCH",
							 //"txAdmUrl::bg/bg01/selectAcntList.do",
							 "txAdmUrl::com/admi/selectAcntList.do",
							 "",
							 "ds_acntList=resultList",
							 strArg,
							 "fn_callBack");
	}
}

