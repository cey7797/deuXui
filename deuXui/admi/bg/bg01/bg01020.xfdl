/*********************************************************************
 * 함 수 명 	: fn_preSave
 * 함수설명 	: DataSet을 저장 전 처리 함수.
 *********************************************************************/
this.fn_preSave = function()
{
	if(!this.gfn_isUpdate(this.ds_acntList) && !this.gfn_isUpdate(this.ds_acntSubList) ){
		this.gfn_message("A", "변경된 데이터가 없습니다.");
		return false;
	}

	/*
	 * 필수값 입력 체크
	 */
	var colNm = "acntUnt$acntYy$acntCd$acntNm$useYn";
	var korNm = "회계단위$회계년도$계정과목코드$계정명$사용여부";
	var cmpNm = "$";
	
	var result = this.gfn_chkEssItem("ds_acntList", colNm, korNm, cmpNm);
	if (result[0] != "success") {
		this.gfn_message("A", result[0]);
		this.ds_acntList.set_rowposition(result[1]);
		if(result[3] != ""){
			this.objects[result[3]].setFocus();
		}
		return false;	
	}
	
	/*
	 * 필수값 입력 체크
	 */
	var colNm = "acntUnt$acntYy$acntCd$balDiv$itmSeqNm$useYn";
	var korNm = "회계단위$회계년도$계정과목코드$수지구분$세세목명$사용여부";
	var cmpNm = "$";
	
	var result = this.gfn_chkEssItem("ds_acntSubList", colNm, korNm, cmpNm);
	if (result[0] != "success") {
		this.gfn_message("A", result[0]);
		this.ds_acntSubList.set_rowposition(result[1]);
		if(result[3] != ""){
			this.objects[result[3]].setFocus();
		}
		return false;	
	}
		
	//데이터 중복 체크
	if(!this.gfn_chkValidation("ds_acntList", "acntYy$acntUnt$acntCd", "회계년도, 회계단위, 계정코드")){
		return false;
	}
		
	return true;
}

