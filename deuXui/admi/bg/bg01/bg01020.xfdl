 *********************************************************************/
this.ds_acntList_onrowposchanged = function(obj:Dataset, e:nexacro.DSRowPosChangeEventInfo)
{
	//선택한 행의 레벨 값
	var strLvl = this.ds_acntList.getColumn(this.ds_acntList.rowposition,"acntLvl");
	
	//계정레벨이 '장'일때는 상위계정코드를 비활성화 한다.
	if(strLvl == "10") {
		this.div_detail.btnPopUprAcnt.set_visible(false); this.div_detail.btnPopUprAcnt.set_enable(false);
		this.div_detail.edtUprAcntCd.set_visible(false);
	} else {
		this.div_detail.btnPopUprAcnt.set_visible(true); this.div_detail.btnPopUprAcnt.set_enable(true);
		this.div_detail.edtUprAcntCd.set_visible(true);
	}
	
	//계정레벨이 '세목'일때만 활성화/조회 한다.
	if(strLvl == "50") {
		this.Div00.btn_newSub.set_visible(true); this.Div00.btn_newSub.set_enable(true);
		this.Div00.btn_delSub.set_visible(true); this.Div00.btn_delSub.set_enable(true);
		this.fn_searchSub();
	} else {
		this.Div00.btn_newSub.set_visible(false); this.Div00.btn_newSub.set_enable(false);
		this.Div00.btn_delSub.set_visible(false); this.Div00.btn_delSub.set_enable(false);
		this.ds_acntSubList.clearData();
	}	
}

