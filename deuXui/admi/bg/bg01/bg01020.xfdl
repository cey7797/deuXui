 * ==================================================================== */

/*********************************************************************
 * 함 수 명 	: fn_callBack
 * 함수설명 	: 서버 통신 후 callBack 함수.
 *********************************************************************/
this.fn_callBack = function(trId, errCd, errMsg)
{
	if(errCd < 0){
		this.gfn_message("A", errMsg);
	}else{
		if(trId=="LOAD"){
			this.fn_postLoad();
		} else if(trId == "SEARCH"){
			this.fn_postSearch();
		} else if(trId == "SEARCHSUB"){
			this.fn_postSearchSub();
		} else if(trId == "SAVE"){
			this.fn_postSave();
		} else if(trId == "ACNTUNTCOMBO") {
			this.div_search.cmb_schAcntUnt.set_index(0);
			this.ds_pfmAcntUnt.assign(this.ds_schAcntUnt);
		} else if(trId == "SAVECOPY"){
			this.fn_postSave();
		}	
	}
}

/*********************************************************************
 * 함 수 명 	: fn_popCallback
 * 함수설명 	: 팝업종료 후 callBack 함수.
 *********************************************************************/
this.fn_popCallback = function (strID, rtnVal)
{
	if(rtnVal == "close") {
		return;
	}
	this.ds_rtnVal.clear();
	this.ds_rtnVal.loadXML(rtnVal);
	
	if(this.ds_rtnVal.rowcount == 0) return false;
	
	//상위계정코드팝업
	if(strID == "admi01010_p02_upr") {
		this.div_detail.edtUprAcntCd.set_value(this.ds_rtnVal.getColumn(0,"acntCd"));
	} 
	//전년도계정코드팝업
	else if(strID == "admi01010_p02_bf") {
		this.div_detail.edtBfAcntCd.set_value(this.ds_rtnVal.getColumn(0,"acntCd"));
	}
}]]></Script>
