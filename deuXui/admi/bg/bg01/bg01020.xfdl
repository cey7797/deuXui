{
	if(this.fn_preSave()){
		this.gfn_transaction("SAVE",
							 //"txAdmUrl::bg/bg01/saveAcntList.do",
							 "txAdmUrl::com/admi/saveAcntList.do",
							 "in_acntList=ds_acntList:u in_acntSubList=ds_acntSubList:u",
							 "",
							 "",
							 "fn_callBack");
	}
}

