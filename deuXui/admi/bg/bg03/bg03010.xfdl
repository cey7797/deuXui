	var popupUrl;
	if(this.div_search.cmb_chgDiv.value == "10"){
		popupUrl = "bg03010_p03";
	}else if(this.div_search.cmb_chgDiv.value == "20"){
		popupUrl = "bg03010_p04";
	}
	
	var argumentsList = new Array();
		argumentsList['acntYy']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"acntYy");
		argumentsList['acntUnt']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"acntUnt");
		argumentsList['aplyOrgId']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"aplyOrgId");
		argumentsList['aplyOrgNm']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"aplyOrgNm");
		argumentsList['bizCd']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"bizCd");
		argumentsList['bizNm']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"bizNm");
		argumentsList['adjOrdCd']   = "101";
		argumentsList['pgmId']   = "bg03010";
		argumentsList['schedChk']   = "Y";
		argumentsList['staCd']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"staCd");
		argumentsList['chgSeq']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"chgSeq");
		argumentsList['chgDiv']   = this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"chgDiv");

	var popupId = popupUrl;
	var popupUrl = "bg03::"+popupUrl+".xfdl";
