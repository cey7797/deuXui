/*********************************************************************
 * 함 수 명 	: fn_preSave
 * 함수설명 	: DataSet을 저장 전 처리 함수.
 *********************************************************************/
this.fn_preSave = function()
{
	if(!this.gfn_isUpdate(this.ds_bgChgAply)){
		this.gfn_message("A", "변경된 데이터가 없습니다.");
		return false;
	}
	return true;
}

/*********************************************************************
 * 함 수 명 	: fn_save
 * 함수설명 	: DataSet을 저장한다.
 *********************************************************************/
this.fn_save = function()
{
	if(this.fn_preSave()){
	
		this.gfn_transaction("SAVE",
							 "txAdmUrl::com/admi/saveBgChgAplyList.do",
							 "resultList=ds_bgChgAply:u",
							 "",
							 "",
							 "fn_callBack"
							 ,false);
	}
}

/*********************************************************************
 * 함 수 명 	: fn_postSave
 * 함수설명 	: DataSet을 저장 후 처리 함수.
 *********************************************************************/
this.fn_postSave = function()
{
	this.gfn_message("A", "저장이 완료되었습니다.");
	this.fn_search();
}

/*********************************************************************
 * 함 수 명 	: fn_callBack
 * 함수설명 	: 서버 통신 후 callBack 함수.
 *********************************************************************/
this.fn_callBack = function(trId, errCd, errMsg)
{
	if(errCd < 0){
		this.gfn_message("A", errMsg);
	}else{
		if(trId=="LOAD"){
			this.fn_postLoad();
		} else if(trId == "SEARCH"){
			this.fn_postSearch();
		} else if(trId == "SAVE"){
			this.fn_postSave();
		} else if(trId == "ACNTYY"){
			this.fn_postAcntYy();
		} else if(trId == "ACNTUNTCOMBO"){
			this.div_search.cmb_acntUnt.set_index(0);
			this.cfn_BudgOrgIdCombo("ds_budgOrgId",this.div_search.cmb_acntUnt.value);
		} else if(trId == "BUDGORGIDCOMBO"){
			this.div_search.cmb_aplyOrgId.set_index(0);
		} else if(trId == "SAVESTACD"){
			this.gfn_message("A", "처리되었습니다.");
			this.fn_search();
		} else if(trId == "APPROVAL"){
			this.fn_postApproval();
		} else if(trId == "APPROVALCANCEL"){
			this.gfn_message("A", "처리되었습니다.");
			this.fn_search();
		} else if(trId == "COPY") {
			this.fn_postCopy();
		}
	}
}

