/*********************************************************************
 * 함 수 명 	: fn_preApproval
 * 함수설명 	: DataSet을 저장 전 처리 함수.
 *********************************************************************/
this.fn_preApproval = function()
{		
	var nRow = this.ds_bgChgAply.rowposition;

	if(this.ds_bgChgAply.rowposition < 0){
		this.gfn_message("A", "전자결재를 신청할 내역이 없습니다.");
		return false;
	}
	//작성상태가 변경됨.
	//예산변경신청이 입력되면   [작성 : 10]
	//전자결재 기안이 되면      [결재중 : 20]
	//전자결재 기안취소가 되면  [결재반려 : 30]
	//전자결재 기안완료되고 신청이 되었으나, 예산관리자에 의해 반려당하면 [예산반려 : 40]
	//전자결재 기안완료가 되면  [신청 : 50]
	//예산변경 확정이되면        [확정 : 60]
	
	if(this.ds_bgChgAply.getColumn(nRow, "staCd") == "20"){
		this.gfn_message("A", "[결재중]에서는 전자결재 처리를 할 수 없습니다.");
		return false;
	}else if(this.ds_bgChgAply.getColumn(nRow, "staCd") == "50"){
		this.gfn_message("A", "[신청]에서는 전자결재 처리를 할 수 없습니다.");
		return false;
	}else if(this.ds_bgChgAply.getColumn(nRow, "staCd") == "60"){
		this.gfn_message("A", "[확정]에서는 전자결재 처리를 할 수 없습니다.");
		return false;
	}		
	
	if(this.ds_bgChgAplyDet.rowcount == 0 ) {
		this.gfn_message("A", "변경신청 상세 내역이 없습니다.");
		return false;
	}
	
	return true;
}

