this.bg03010_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var arrDataSet = Array(4); 
		arrDataSet[0]  = "ds_budgCd";  			// 예산구분
		arrDataSet[1]  = "ds_chgDiv";  	    // 변경구분
