/********************************************************************************
 * 함 수 명 	: fn_search
 * 함수설명 	: 조회 함수
********************************************************************************/
this.fn_search = function()
{
	if(this.fn_preSearch()){
	
		this.ds_bgChgAplyDet.clearData();
		this.ds_bgChgAply.clearData();

		var strArg = "acntYy=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.spn_acntYy.value));
			strArg += " acntUnt=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.cmb_acntUnt.value));
			strArg += " aplyOrgId=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.cmb_aplyOrgId.value));
			strArg += " staCd=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.cmb_staCd.value));
			strArg += " chgDiv=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(this.div_search.cmb_chgDiv.value));
			strArg += " adjOrdCd=" +  nexacro.wrapQuote("101");
		
		this.gfn_transaction("SEARCH",
							 "txAdmUrl::com/admi/selectBgChgAplyList.do",
							 "",
							 "ds_bgChgAply=resultList",
							 strArg,
							 "fn_callBack");
	}
}

