this.btn_copy_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if ( this.ds_bgChgAply.rowposition < -1 ) {
		this.gfn_message("A", "선택된 자료가 없습니다.");
		return;
	}
	
	if ( this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition, "rowStatus") == 2 ) {
		this.gfn_message("A", "저장후 복사할 수 있습니다.");
		return;
	}
	
	if(!this.gfn_message("C", "선택된 자료를 복사하시겠습니까?")) {
		return;
	}
	
	this.ds_copy.clearData();
	this.ds_copy.insertRow();
	this.ds_copy.setColumn(0, "acntUnt", this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition, "acntUnt"));
	this.ds_copy.setColumn(0, "acntYy", this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition, "acntYy"));
	this.ds_copy.setColumn(0, "aplyOrgId", this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition, "aplyOrgId"));
	this.ds_copy.setColumn(0, "chgSeq", this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition, "chgSeq"));
	
	this.gfn_transaction("COPY",
						 "txAdmUrl::com/admi/copyBgChgAplyList.do",
						 "resultList=ds_copy:u",
						 "",
						 "",
						 "fn_callBack");
}

this.fn_postCopy = function()
{
	this.gfn_message("A", "복사가 완료되었습니다.");
	
	this.fn_search();
