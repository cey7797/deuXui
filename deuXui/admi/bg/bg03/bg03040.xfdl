/********************************************************************************
 * 함 수 명 	: fn_preDelDetBtn
 * 함수설명 	: 삭제 전 처리 함수
********************************************************************************/
this.fn_preDelDetBtn = function(){
	if ( this.ds_bgChg.getCaseCount("chk=='1'") == 0 ) {
		this.gfn_message("A", "선택된 항목이 없습니다.");
		return;
	}
	
	for(var i=this.ds_bgChg.rowcount-1; i>=0; i--) {
		if ( this.ds_bgChg.getColumn(i, "rowStatus") == "2" && this.ds_bgChg.getColumn(i, "chk") == "1" ) {
			this.ds_bgChg.deleteRow(i);
		}
	}
	
	if ( this.ds_bgChg.getCaseCount("chk=='1'") == 0 ) {
		this.gfn_message("A", "삭제되었습니다.");
		return "0";
	}
	
	var strMsg = "삭제하시겠습니까?";
		if(!this.gfn_message("C", strMsg )) {
			return "0";
		}
	
	return "1";
}

/*********************************************************************
 * 함 수 명 	: btn_delDet_onclick
 * 함수설명 	: 선택한 Data를 삭제한다.
 *********************************************************************/
this.btn_delDet_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(this.fn_preDelDetBtn() == "1" ){	
		this.ds_bgChgCopy.clearData();
		for( var i=0; i < this.ds_bgChg.rowcount; i++){
			if(this.ds_bgChg.getColumn(i,"chk") == "1"){
				var nRow = this.ds_bgChgCopy.addRow();
				this.ds_bgChgCopy.copyRow(nRow,this.ds_bgChg,i);
				
			}
		}	
		this.fn_saveBgChgDel();
	}
}

/*********************************************************************
 * 함 수 명 	: btn_saveDet_onclick
 * 함수설명 	: 선택한 Data를 저장한다.
 *********************************************************************/
this.btn_saveDet_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	if(!this.gfn_chkValidation("ds_bgChg","balDiv$bizCd$budgOrgId$acntCd","수지구분,사업코드,예산부서,계정과목")){
		return false;
	}
	
	if(this.ds_bgChg.getCaseCount(this.ds_bgChg.getCaseCount("acntCd=='' || acntCd==null ") > 0 )){
		var strMsg = "계정과목이 입력되지 않은 행이 존재합니다. ";
		this.gfn_message("A", strMsg);
		var nRow = this.ds_bgChg.findRow("acntCd", "");
		if ( nRow >= 0 ) {
			this.ds_bgChg.set_rowposition(nRow);
		} else {
			nRow = this.ds_bgChg.findRow("acntCd", null);
			if ( nRow >= 0 ) {
				this.ds_bgChg.set_rowposition(nRow);
			}
		}
		return false;
	}else if(this.ds_bgChg.getCaseCount(this.ds_bgChg.getCaseCount("bizCd=='' || bizCd==null ") > 0 )){
		var strMsg = "사업명이 입력되지 않은 행이 존재합니다. ";
		this.gfn_message("A", strMsg);
		var nRow = this.ds_bgChg.findRow("bizCd", "");
		if ( nRow >= 0 ) {
			this.ds_bgChg.set_rowposition(nRow);
		} else {
			nRow = this.ds_bgChg.findRow("bizCd", null);
			if ( nRow >= 0 ) {
				this.ds_bgChg.set_rowposition(nRow);
			}
		}
		return false;
	}else if(this.ds_bgChg.getCaseCount(this.ds_bgChg.getCaseCount("budgOrgId=='' || budgOrgId==null ") > 0 )){
		var strMsg = "부서명이 입력되지 않은 행이 존재합니다. ";
		this.gfn_message("A", strMsg);
		var nRow = this.ds_bgChg.findRow("budgOrgId", "");
		if ( nRow >= 0 ) {
			this.ds_bgChg.set_rowposition(nRow);
		} else {
			nRow = this.ds_bgChg.findRow("budgOrgId", null);
			if ( nRow >= 0 ) {
				this.ds_bgChg.set_rowposition(nRow);
			}
		}
		return false;
	}else if(this.ds_bgChg.getCaseCount(this.ds_bgChg.getCaseCount("budgAmt=='' || budgAmt==null ") > 0 )){
		var strMsg = "예산신청액이 입력되지 않은 행이 존재합니다. ";
		this.gfn_message("A", strMsg);
		var nRow = this.ds_bgChg.findRow("budgAmt", "");
		if ( nRow >= 0 ) {
			this.ds_bgChg.set_rowposition(nRow);
		} else {
			nRow = this.ds_bgChg.findRow("budgAmt", null);
			if ( nRow >= 0 ) {
				this.ds_bgChg.set_rowposition(nRow);
			}
		}
		return false;
	}else{
		if ( this.div_search.cmb_chgDiv == "10" ) {
			if(nexacro.toNumber(this.ds_bgChg.getSum("budgAmt")) != 0  ){
				alert("예산신청액의 총합계가 0원일때만 저장이 가능합니다");
				return false;
			}else{
				//일괄저장 / 일괄삭제 / 계정코드일괄변경을 위해 유지
				this.fn_saveAcctAll(this.ds_bgChg.rowposition,1); //저장
			}		
		} else {
			if(nexacro.toNumber(this.ds_bgChg.getCaseSum("balDiv == '10'","budgAmt")) != this.ds_bgChg.getCaseSum("balDiv == '20'","budgAmt")  ){
				alert("수입과 지출의 금액이 같아야만 저장이 가능합니다.");
				return false;
			}else{
				//일괄저장 / 일괄삭제 / 계정코드일괄변경을 위해 유지
				this.fn_saveAcctAll(this.ds_bgChg.rowposition,1); //저장
			}				
		}
	}

}

