		}
		var strArg = {jojikCdNm:this.gfn_getNullToEmpty(this.ds_bgChgAply.getColumn(this.ds_bgChgAply.rowposition,"budgOrgNm"))
					 ,autoSearch:true
					 ,autoClose:false}
		this.cfn_deptPopup(strArg,"fn_bgDeptPopCallback");
	}
	
}

