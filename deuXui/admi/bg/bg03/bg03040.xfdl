 *********************************************************************/
this.div_search_cmb_staCd_onitemchanged = function(obj:Combo, e:nexacro.ItemChangeEventInfo)
{
	this.fn_btnCont();
	
	this.ds_bgChg.clearData();
	this.ds_bgChgAply.clearData();
}

/********************************************************************************
 * 함 수 명 	: fn_btnCont
 * 함수설명 	: 작성상태에 따라 버튼컨트롤한다.
********************************************************************************/
this.fn_btnCont = function(){
	//작성상태가 변경됨.
	//예산변경신청이 입력되면   [작성 : 10]
	//전자결재 기안이 되면      [결재중 : 20]
	//전자결재 기안취소가 되면  [결재반려 : 30]
	//전자결재 기안완료되고 신청이 되었으나, 예산관리자에 의해 반려당하면 [예산반려 : 40]
	//전자결재 기안완료가 되면  [신청 : 50]
	//예산변경 확정이되면        [확정 : 60]
	
	if(this.div_search.cmb_staCd.value == "10"){ //작성
		if ( this.div_search.cmb_chgDiv.value == "10" ) {
			this.btn_dept10.set_enable(true);
			this.btn_dept20.set_enable(true);
		} else {
			this.btn_dept10.set_enable(false);
			this.btn_dept20.set_enable(false);
		}
		this.btn_del.set_enable(true);
		this.btn_newDet.set_enable(true);
		this.btn_delDet.set_enable(true);
		this.btn_saveDet.set_enable(true);
		this.grd_det.set_readonly(false);
		this.grd_det.set_enableevent(true);
		this.btn_apply.set_enable(true);
		this.btn_apply.set_text("예산확정");
	}else if(this.div_search.cmb_staCd.value == "60"){//확정
		this.btn_del.set_enable(false);
		this.btn_dept10.set_enable(false);
		this.btn_dept20.set_enable(false);
		this.btn_newDet.set_enable(false);
		this.btn_delDet.set_enable(false);
		this.btn_saveDet.set_enable(false);
		this.grd_det.set_readonly(true);
		this.grd_det.set_enableevent(false);
		this.btn_apply.set_enable(true);
		this.btn_apply.set_text("확정취소");
	}else{
		this.btn_dept10.set_enable(false);
		this.btn_dept20.set_enable(false);
		this.btn_newDet.set_enable(false);
		this.btn_delDet.set_enable(false);
