/*********************************************************************
 * 함 수 명 	: fn_callBack
 * 함수설명 	: 서버 통신 후 callBack 함수.
 *********************************************************************/
this.fn_callBack = function(trId, errCd, errMsg)
{
	if(errCd < 0){
		this.gfn_message("A", errMsg);
	}else{
		if(trId=="LOAD"){
			this.fn_postLoad();
		} else if(trId == "SEARCH"){
			this.fn_postSearch();
		} else if(trId == "SAVE"){
			this.fn_postSave();
		} else if(trId == "SEARCHDET"){
			this.fn_postSearchDet();
		} else if(trId == "SAVEBGCHGDEL"){
			this.fn_postSaveBgChgDel();
		} else if(trId == "SAVEBGCHG"){
			this.fn_postSaveBgChg();		
		} else if(trId == "ACNTYY"){
			this.fn_postAcntYy();
		} else if(trId == "ACNTUNTCOMBO"){
			this.div_search.cmb_acntUnt.set_index(0);
		}else if(trId == "SAVESTACDRECA"){
			alert("반려되었습니다.");
			this.fn_search();
		}else if(trId == "SAVESTACD"){
			alert("처리되었습니다.");
			this.fn_search();
		} else if (trId == "COPY") {
			this.fn_postCopy();
		}
	}
}

