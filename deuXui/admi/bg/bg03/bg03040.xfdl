		this.grd_det.set_formatid("20");
	}
}

/*********************************************************************
 * 함 수 명 	: fn_dept_add
 * 함수설명 	: 변경신청 상세 내역중 대량의 학과,학부를
 * 				  일괄로 등록하기 위한 함수
 *********************************************************************/
this.fn_dept_add = function(gb)
{
	var strArg = "gb=" +  nexacro.wrapQuote(this.gfn_getNullToEmpty(gb));

	this.gfn_transaction("SEARCHDEPT",
						 "txAdmUrl::com/admi/selectBgChgDeptAdddList.do",
						 "",
						 "ds_dept=resultList",
						 strArg,
						 "fn_callBack",
						 false);
	//trace(this.ds_dept.saveXML());	

	for(i=0; i<=this.ds_dept.rowcount; i++){
		var nRow = this.ds_bgChg.addRow();
		this.ds_bgChg.setColumn(nRow,"rowStatus",2);
		
		this.ds_bgChg.setColumn(nRow,"acntUnt",this.ds_bgChg.getColumn(i,"acntUnt"));
		this.ds_bgChg.setColumn(nRow,"acntYy",this.ds_bgChg.getColumn(i,"acntYy"));
		this.ds_bgChg.setColumn(nRow,"aplyOrgId",this.ds_bgChg.getColumn(i,"aplyOrgId"));
		this.ds_bgChg.setColumn(nRow,"chgSeq",this.ds_bgChg.getColumn(i,"chgSeq"));
		this.ds_bgChg.setColumn(nRow,"adjOrdCd",101);		
		
		this.ds_bgChg.setColumn(nRow,"balDiv",this.ds_bgChg.getColumn(i,"balDiv"));
		this.ds_bgChg.setColumn(nRow,"acntCd",this.ds_bgChg.getColumn(i,"acntCd"));
		this.ds_bgChg.setColumn(nRow,"acntNm",this.ds_bgChg.getColumn(i,"acntNm"));
		this.ds_bgChg.setColumn(nRow,"bizCd",this.ds_bgChg.getColumn(i,"bizCd"));
		this.ds_bgChg.setColumn(nRow,"bizNm",this.ds_bgChg.getColumn(i,"bizNm"));
		
// 		this.ds_bgChg.setColumn(i,"totalBudgAmt",this.ds_bgChg.getColumn(i-1,"totalBudgAmt"));
// 		this.ds_bgChg.setColumn(i,"execAmt",this.ds_bgChg.getColumn(i-1,"execAmt"));
// 		this.ds_bgChg.setColumn(i,"balnAmt",this.ds_bgChg.getColumn(i-1,"balnAmt"));
// 		this.ds_bgChg.setColumn(i,"budgAmt",this.ds_bgChg.getColumn(i-1,"budgAmt"));
		
		this.ds_bgChg.setColumn(nRow,"budgOrgNm",this.ds_dept.getColumn(i,"deptNmKor"));
		this.ds_bgChg.setColumn(nRow,"budgOrgId",this.ds_dept.getColumn(i,"deptCd"));
		this.ds_bgChg.setColumn(nRow,"upDeptNm",this.ds_dept.getColumn(i,"upDeptNm"));
		
		if(this.ds_bgChg.getColumn(nRow,"budgOrgId") == "" || this.ds_bgChg.getColumn(nRow,"budgOrgId") == null){
			this.ds_bgChg.deleteRow(nRow);
		}
		
	}					 
}

this.pre_btn_dept = function() 
{
	if(this.ds_bgChgAply.rowposition < 0 ) {
		var strMsg = "변경신청내역이 없습니다.";
		this.gfn_message("A",strMsg);
		return false;
	}
	
	if(this.ds_bgChg.rowcount <= 0 ){
		var strMsg = "학부/학과를 추가하실땐 선행데이타(1개의 행만 추가)으로 입력 해 주세요.";
		this.gfn_message("A",strMsg);
		return false;
	}
	
	return true;
		
}

