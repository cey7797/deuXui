/********************************************************************************
 * 함 수 명 	: btn_apply_onclick
 * 함수설명 	: 편성된 예산을 변경신청한다.
********************************************************************************/
this.btn_apply_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//작성상태가 변경됨.
	//예산변경신청이 입력되면   [작성 : 10]
	//전자결재 기안이 되면      [결재중 : 20]
	//전자결재 기안취소가 되면  [결재반려 : 30]
	//전자결재 기안완료되고 신청이 되었으나, 예산관리자에 의해 반려당하면 [예산반려 : 40]
	//전자결재 기안완료가 되면  [신청 : 50]
	//예산변경 확정이되면        [확정 : 60]
	if(this.btn_apply.text == "확정취소"){
		bgStaCdApply = "50";
		cfmUserId = "";
		cfmDt = "";

		if ( this.ds_bgChgAply.getCaseCount("chk=='1'") == 0 ) {
			this.gfn_message("A", "선택된 항목이 없습니다.");
			return;
		}
		
		var strMsg = "선택된 예산을 취소합니다.";
		if(!this.gfn_message("C", strMsg)) {
			return;
		}
		
		this.ds_bgChgAplyCopy.clearData();
		for( var i=0; i < this.ds_bgChgAply.rowcount; i++){
			if(this.ds_bgChgAply.getColumn(i,"chk") == "1"){
				var nRow = this.ds_bgChgAplyCopy.addRow();
				this.ds_bgChgAplyCopy.copyRow(nRow,this.ds_bgChgAply,i);
			}
		}
		this.fn_saveStaCd();
	}else{
		bgStaCdApply = "60";
		cfmUserId = application.gds_user.getColumn(application.gds_user.rowposition,"memberNo");
		cfmDt = this.cal_fixDate.value;
		
		if ( this.ds_bgChgAply.getCaseCount("chk=='1'") == 0 ) {
			this.gfn_message("A", "선택된 항목이 없습니다.");
			return;
		}
		
		for( var i=0; i < this.ds_bgChgAply.rowcount; i++){
			if(this.ds_bgChgAply.getColumn(i,"chk") == "1"){
				if(!this.gfn_isNull(this.ds_bgChgAply.getColumn(i,"cfmDt")) && !this.gfn_isNull(this.ds_bgChgAply.getColumn(i,"cfmUserId")) ){
					this.gfn_message("A",  "확정된 예산은 재확정이 불가능합니다.");
					return false;
				}
			}
		}

		var strMsg = "선택된 예산을 확정합니다.";
		if(!this.gfn_message("C", strMsg)) {
			return;
		}

		
		this.ds_bgChgAplyCopy.clearData();
		for( var i=0; i < this.ds_bgChgAply.rowcount; i++){
			if(this.ds_bgChgAply.getColumn(i,"chk") == "1"){
				var nRow = this.ds_bgChgAplyCopy.addRow();
				this.ds_bgChgAplyCopy.copyRow(nRow,this.ds_bgChgAply,i);	
			} 
		}
		//this.fn_preSaveStaCd();
		this.fn_saveStaCd();
	}
}

/*********************************************************************
 * 함 수 명 	: fn_preSaveStaCd
 * 함수설명 	: 예산신청하기전(금액비교체크후) 빈게정과목체크 
 *********************************************************************/
this.fn_preSaveStaCd = function()
{
	for(i=0; i < this.ds_bgFrmnApplyCopy.rowcount; i++){
			var strArg  = "acntUnt="  + nexacro.wrapQuote(this.ds_bgChgAplyCopy.getColumn(i,"acntUnt"));
				strArg  += "acntYy="  + nexacro.wrapQuote(this.ds_bgChgAplyCopy.getColumn(i,"acntYy"));
				strArg  += "chgSeq="  + nexacro.wrapQuote(this.ds_bgChgAplyCopy.getColumn(i,"chgSeq"));
				strArg  += "budgOrgId="  + nexacro.wrapQuote(this.ds_bgChgAplyCopy.getColumn(i,"budgOrgId"));
				strArg  += "bizCd="  + nexacro.wrapQuote(this.ds_bgChgAplyCopy.getColumn(i,"bizCd")); //안씀
				strArg  += "balDiv="  + nexacro.wrapQuote(this.ds_bgChgAplyCopy.getColumn(i,"balDiv"));
				strArg  += "staCd="  + nexacro.wrapQuote(this.ds_bgChgAplyCopy.getColumn(i,"staCd"));
			this.gfn_transaction("SAVEAPPLYACNTCHK",
								 "txComUrl::com/admi/selectBgChgApplyAcntChk.do",
								 "",
								 "ds_bgApplyAcntChk=resultList",
								 strArg,
								 "fn_callBack",
								 false);//false

		 for( var j = i; j < this.ds_bgApplyAcntChk.rowcount; j++){
			if( nexacro.toNumber(this.ds_bgApplyAcntChk.getColumn(j,"bfrBudgAmt")) <= 0 ){
				var strMsg = "미입력된 계정과목이 존재합니다.\n\n";
					strMsg += "회계연도 : "+this.ds_bgApplyAcntChk.getColumn(j,"acntYy")+".\n\n";
					strMsg += "회계단위명 : "+this.ds_bgApplyAcntChk.getColumn(j,"acntUntNm")+".\n\n";
					strMsg += "사업명 : "+this.ds_bgApplyAcntChk.getColumn(j,"bizNm")+".\n\n";
					strMsg += "계정과목명 : "+this.ds_bgApplyAcntChk.getColumn(j,"bfrAcntNm")+".";
					this.gfn_message("A",  strMsg);
					return false;
			}
		}
	}
	this.fn_saveStaCd();
}

/*********************************************************************
 * 함 수 명 	: fn_saveStaCd
 * 함수설명 	: DataSet을 저장한다.
 *********************************************************************/
this.fn_saveStaCd = function()
{
	for( var i=0; i < this.ds_bgChgAplyCopy.rowcount; i++){
		this.ds_bgChgAplyCopy.setColumn(i,"rowStatus",4);
		this.ds_bgChgAplyCopy.setColumn(i,"staCd",bgStaCdApply);
		this.ds_bgChgAplyCopy.setColumn(i,"cfmUserId",cfmUserId); 
		this.ds_bgChgAplyCopy.setColumn(i,"cfmDt",cfmDt); 
		this.ds_bgChgAplyCopy.setColumn(i,"adjOrdCd","101"); 
		//this.ds_bgChgAplyCopy.setColumn(i,"aplyOrgId",); 
	}
	
	this.gfn_transaction("SAVESTACD",
						 "txComUrl::com/admi/saveBgChgStaCdFixed.do",
						 "resultList=ds_bgChgAplyCopy:u",
						 "",
						 "",
						 "fn_callBack");
}

