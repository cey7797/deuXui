/********************************************************************************
 * 함 수 명 	: fn_btnCont
 * 함수설명 	: 작성상태에 따라 버튼컨트롤한다.
********************************************************************************/
this.fn_btnCont = function(){
	//작성상태가 변경됨.
	//예산변경신청이 입력되면   [작성 : 10]
	//전자결재 기안이 되면      [결재중 : 20]
	//전자결재 기안취소가 되면  [결재반려 : 30]
	//전자결재 기안완료되고 신청이 되었으나, 예산관리자에 의해 반려당하면 [예산반려 : 40]
	//전자결재 기안완료가 되면  [신청 : 50]
	//예산변경 확정이되면        [확정 : 60]
	
	if(this.div_search.cmb_staCd.value == "10"){ //작성
		this.btn_apply.set_enable(false);
		this.btn_apply.set_text("예산확정");
		this.btn_reca.set_enable(false);
	}else if(this.div_search.cmb_staCd.value == "20"){//결재중
		this.btn_apply.set_enable(false);
		this.btn_apply.set_text("예산확정");
		this.btn_reca.set_enable(false);
	}else if(this.div_search.cmb_staCd.value == "30"){//결재반려
		this.btn_apply.set_enable(false);
		this.btn_apply.set_text("예산확정");
		this.btn_reca.set_enable(false);
	}else if(this.div_search.cmb_staCd.value == "40"){//예산반려
		this.btn_apply.set_enable(false);
		this.btn_apply.set_text("예산확정");
		this.btn_reca.set_enable(false);	
	}else if(this.div_search.cmb_staCd.value == "50"){//신청
		this.btn_apply.set_enable(true);
		this.btn_apply.set_text("예산확정");
		this.btn_reca.set_enable(true);
	}else if(this.div_search.cmb_staCd.value == "60"){//확정
		this.btn_apply.set_enable(true);
		this.btn_apply.set_text("확정취소");
		this.btn_reca.set_enable(false);
	}else{
		this.btn_apply.set_enable(false);
		this.btn_apply.set_text("예산확정");
		this.btn_reca.set_enable(false);
	}
	
}

