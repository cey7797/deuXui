/*********************************************************************
 * 함 수 명 	: fn_callBack
 * 함수설명 	: 서버 통신 후 callBack 함수.
 *********************************************************************/
this.fn_callBack = function(trId, errCd, errMsg)
{
	if(errCd < 0){
		this.gfn_message("A", errMsg);
	}else{
		if(trId=="LOAD"){
			this.fn_postLoad();
		}else if(trId == "FIND"){
			this.fn_postFind();
		}else if(trId == "FINDDET"){
			this.fn_postFindDet();
		}else if(trId == "SAVE"){
			this.fn_postSave();
		}else if(trId == "SAVEREGI"){
			this.fn_postSave();
		}else if(trId == "SAVENONREGI"){
			this.fn_postSave();
		}else if(trId == "SAVEROLE"){
			this.fn_postSaveRole();
		}else if(trId == "SYS"){
			this.div_search.cmb_sysId.set_index(0);
			this.gfn_findSubCombo(this.div_search.cmb_sysId.value, "T");
		}else if(trId == "SUB"){
			this.div_search.cmb_subId.set_index(0);
			this.gfn_findMenuCombo(this.div_search.cmb_sysId.value, this.div_search.cmb_subId.value, "T");
		}else if(trId == "MENU"){	
			this.div_search.cmb_menuId.set_index(0);
		}else if(trId = "ROLE"){
			this.fn_postFindRole();
		}
	}
}

