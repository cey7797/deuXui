this.ds_detail_tree_oncolumnchanged = function(obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var curTreeId = "";
	var upSysId = "";
	var curTreeIdLow = "";
	var upSysIdLow = "";
	var leaf = "";
	
	//최하위일 경우 체크만 함
	if(obj.getColumn(obj.rowposition, 'leaf')=='1'){
		return false;
	}
	
	//그리드 체크 했을 때
	if( obj.getColumn(obj.rowposition, e.columnid) == '1'){

//		obj.set_enableevent(false);

		curTreeId = obj.getColumn(obj.rowposition, 'treeId');

		for(var i=0; i<obj.getRowCount(); i++){
			var connPath = obj.getColumn(i, 'connPath');
			if( connPath.indexOf(curTreeId) >= 0 ){
				obj.setColumn(i, e.columnid, '1');
				if(obj.getOrgColumn(i, e.col) != e.newvalue && obj.getColumn(i, 'leaf')=='1'){
					obj.setColumn(i, "rowStatus", 4);
				}else if(obj.getColumn(i, 'leaf')=='0'){
					obj.setColumn(i, "rowStatus", "");
				}else{
					obj.setColumn(i, "rowStatus", "");
				}
			}								
		}
		obj.setColumn(obj.rowposition, "rowStatus", "");
// 		obj.set_enableevent(true);		
		
	}else{//체크 해제 할 때
//		obj.set_enableevent(false);
			
		curTreeId = obj.getColumn(obj.rowposition, 'treeId');

		for(var i=0; i<obj.getRowCount(); i++){
			var connPath = obj.getColumn(i, 'connPath');
			if( connPath.indexOf(curTreeId) >= 0 ){
				obj.setColumn(i, e.columnid, '0');
				if(obj.getOrgColumn(i, e.col) == e.newvalue && obj.getColumn(i, 'leaf')=='1'){
					obj.setColumn(i, "rowStatus", "");
				}else if(obj.getColumn(i, 'leaf')=='0'){
					obj.setColumn(i, "rowStatus", "");
				}else{
					obj.setColumn(i, "rowStatus", 4);
				}
			}								
		}
		obj.setColumn(obj.rowposition, "rowStatus", "");
//		obj.set_enableevent(true);			
	}
}
