/******************************************************************************
* 함 수 명 	: com020003_onload
* 함수설명 	: 화면 로드시 실행 함수
*****************************************************************************/
this.com020003_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	var arrDataSet = Array();
		arrDataSet[0]  = "ds_schUserGb";  			//사용자구분
	
	// 조회조건으로	넘어갈 공통코드	정의.(화면대로 순서꼭 지켜주세요.)	
	var arrGrpCd = Array();
		arrGrpCd[0]  = "USER_GB"; //사용자구분
			
	// 사용여부	지정할 아큐먼트를 넘긴다.
	var arrUseYn =	Array();
		arrUseYn[0]  =	"Y";
	
	// T:전체,S:선택,X:없음 
	var arrModeFg =	Array(); 
		arrModeFg[0]  =	"T";

	// WHERE절 내용 "X":없음
	var arrWhere =	Array();
		arrWhere[0]  =	"X";//ABB_CD IN ('S','K')
		
	/* 메소드명     : gfn_comboLoad
	 * 설명         : 공통코드 테이블을 조회하여 콤보박스를 생성한다.
	 * 메소드인수1  : DataSet명의 배열
	 * 메소드인수2  : 그룹코드의 배열
	 * 메소드인수3  : 조건문의 배열 (1:사용, 0:사용안함)
	 * 메소드인수4  : 전체공란 첨부여부 "T": 전체, "S":선택, "X" : 데이터시작
	 * 메소드인수5  : WHERE절 내용 "X":없음
	 */

 	this.gfn_comboLoad(arrDataSet, arrGrpCd, arrUseYn, arrModeFg, arrWhere);
}

