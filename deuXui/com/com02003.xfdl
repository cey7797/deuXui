/********************************************************************************
 * 함 수 명 	: fn_find
 * 함수설명 	: 미등록정보 조회 함수
********************************************************************************/
this.fn_find = function()
{
	if(this.fn_preFind()){
		var strArg = "pv_roleId="+ nexacro.wrapQuote(this.ds_roleId.getColumn(this.ds_roleId.rowposition,"roleId"));
		
		if(!this.gfn_isNull(this.div_search.cmb_sysId.value)){
			strArg += " pv_sysId="  + nexacro.wrapQuote(this.div_search.cmb_sysId.value);
		}
		if(!this.gfn_isNull(this.div_search.cmb_subId.value)){
		    strArg += " pv_subId=" + nexacro.wrapQuote(this.div_search.cmb_subId.value);
		}
		if(!this.gfn_isNull(this.div_search.cmb_menuId.value)){
		    strArg += " pv_menuId=" + nexacro.wrapQuote(this.div_search.cmb_menuId.value);
		}
		
		this.gfn_transaction("FIND",
							 "txComUrl::com/selectRolePgm.do",
							 "",
							 "ds_main=resultList ds_main_tree=resultList2",
							 strArg,
							 "fn_callBack");
	}
}

