 * 함수설명 	: DataSet을 저장 전 처리 함수.
 *********************************************************************/
this.fn_preSaveRole = function()
{
	if(!this.gfn_isUpdate(this.ds_roleId)){
		this.gfn_message("A", "변경된 데이터가 없습니다.");
		return false;
	}

	//데이터 중복 체크
	if(!this.gfn_chkValidation("ds_roleId", "roleId", "권한ID")){
		return false;
	}
	
	/*
	 * 필수값 입력 체크
	 */
	var colNm = "roleId";
	var korNm = "권한ID";
	var cmpNm = "$";
	
	var result = this.gfn_chkEssItem("ds_roleId", colNm, korNm, cmpNm);
	if (result[0] != "success") {
		this.gfn_message("A", result[0]);
		this.ds_roleId.set_rowposition(result[1]);
		if(result[3] != ""){
			this.objects[result[3]].setFocus();
		}
		return false;	
	}
	
	return true;
}

